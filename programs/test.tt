function testunpack() {
	var err;
	try { call $unpack( except( ${ 1 } ) 0 ); } catch err {}
	call assert( =( $cmp( $str( err ) "1" ) 0 ), ${-} );
	let err = 0;
	try { call $unpack( 0 0 ); } catch err {}
	call assert( err, ${-} );
	try { call $unpack( "" except( ${ 2 } ) ); } catch err {}
	call assert( =( $cmp( $str( err ) "2" ) 0 ), ${-} );
	let err = 0;
	try { call $unpack( "\0\0\0\0" -1 ); } catch err {}
	call assert( err, ${-} );
	let err = 0;
	try { call $unpack( "\0\0\0" 0 ); } catch err {}
	call assert( err, ${-} );
	call assert( =( $unpack( "\0\0\0\1" 0 ) 1 ), ${-} );
	call assert( =( $unpack( "\1\2\3\4" 0 ) 0x1020304 ), ${-} );
	call assert( =( $unpack( $pack( -1 ) 0 ) -1 ), ${-} );
	call assert( =( $unpack( $pack( $array( ${ 1 2 } ) ) 1 ) 2 ), ${-} );
}

function teststruct() {
	call assert( =( struc 5 ), ${-} );
	call assert( =( exten 7 ), ${-} );
	call assert( =( struc.a 0 ), ${-} );
	call assert( =( struc.e 4 ), ${-} );
	call assert( =( exten.g 6 ), ${-} );
	call assert( =( $len( $new( exten ) ) 7 ), ${-} );
}

function testfuncref() {
	var err;
	call assert( =( $cmp( @testfuncref "testfuncref" ) 0 ), ${-} );
	try { call :( $tup( "a" 1 ) ); } catch err {}
	call assert( =( err 1 ), ${-} );
	try { call :( @setg 1 2 ); } catch err {}
	call assert( =( err 2 ), ${-} );
	call setg( 0 );
	call :( @setg 1 );
	call assert( =( g 1 ), ${-} );
}

function testeq() {
	var err;
	call setg( 0 );
	try { call $eq( except( ${ 1 } ) setg( 2 ) ); } catch err {}
	call assert( =( $cmp( $str( err ) "1" ) 0 ), ${-} );
	call assert( =( g 0 ), ${-} );
	let err = 0;
	try { call $eq( 0 except( ${ 2 } ) ); } catch err {}
	call assert( =( $cmp( $str( err ) "2" ) 0 ), ${-} );
	call assert( =( $eq( 0 0 ) 1 ), ${-} );
	call assert( =( $eq( "" "" ) 1 ), ${-} );
	call assert( =( $eq( "a" "a" ) 1 ), ${-} );
	call assert( =( $eq( STR STR ) 1 ), ${-} );
	call assert( =( $eq( @testeq @testeq ) 1 ), ${-} );
	call assert( =( $eq( 0 1 ) 0 ), ${-} );
	call assert( =( $eq( 0 "a" ) 0 ), ${-} );
	call assert( =( $eq( "a" 0 ) 0 ), ${-} );
	call assert( =( $eq( "a" "aa" ) 0 ), ${-} );
}

function testsrc() {
	call assert( $eq( $sub( $src, -( $len( $src ) 7 ), 7 ) "test.tt" ), ${-} );
}

function testchop() {
	var err;
	call setg( 0 );
	try { call $chop( except( ${ 1 } ), setg( 2 ) ); } catch err {}
	call assert( =( $cmp( $str( err ) "1" ) 0 ), ${-} );
	call assert( =( g 0 ), ${-} );
	let err = 0;
	try { call $chop( setg( 1 ), except( ${ 2 } ) ); } catch err {}
	call assert( =( $cmp( $str( err ) "2" ) 0 ), ${-} );
	call assert( =( g 1 ), ${-} );
	let err = 0;
	try { call $chop( 0, "" ); } catch err { let err = 1; }
	call assert( =( err 1 ), ${-} );
	try { call $chop( "", 0 ); } catch err { let err = 1; }
	call assert( =( err 1 ), ${-} );
	call assert( =( $cmp( $chop( "", "" ), "" ) 0 ), ${-} );
	call assert( =( $cmp( $chop( "file", "" ), "" ) 0 ), ${-} );
	call assert( =( $cmp( $chop( "file", "/" ), "" ) 0 ), ${-} );
	call assert( =( $cmp( $chop( "dir/file", "/" ), "dir/" ) 0 ), ${-} );
	call assert( =( $cmp( $chop( "c:file", "/:" ), "c:" ) 0 ), ${-} );
	call assert( =( $cmp( $chop( "dir\\file", "/:\\" ), "dir\\" ) 0 ), ${-} );
	call assert( =( $cmp( $chop( "c:\\dir\\file", "/:\\" ), "c:\\dir\\" ) 0 ), ${-} );
}

function testinterrupted() {
	var x;
	let x = STR;
	let x = $interrupted;
	call assert( !( x ), ${-} );
}

function testinc() {
	var int = "", err;
	try { inc int; } catch err {}
	call assert( err, ${-} );
	let int = 0;
	inc int;
	call assert( =( int 1 ), ${-} );
}

function testdec() {
	var int = "", err;
	try { dec int; } catch err {}
	call assert( err, ${-} );
	let int = 0;
	dec int;
	call assert( =( int -1 ), ${-} );
}

function testelem() {
	var err;
	try { call $elem( except( ${ 1 } ), 0, 0 ); } catch err {}
	call assert( =( $cmp( $str( err ) "1" ) 0 ), ${-} );
	let err = 0;
	try { call $elem( 1, 0, 0 ); } catch err {}
	call assert( =( err 1 ), ${-} );
	let err = 0;
	try { call $elem( ${-}, except( ${ 2 } ), 0 ); } catch err {}
	call assert( =( $cmp( $str( err ) "2" ) 0 ), ${-} );
	let err = 0;
	try { call $elem( ${-}, 2, 0 ); } catch err {}
	call assert( =( err 2 ), ${-} );
	let err = 0;
	try { call $elem( ${-}, 0, except( ${ 3 } ) ); } catch err {}
	call assert( =( $cmp( $str( err ) "3" ) 0 ), ${-} );
	let err = 0;
	try { call $elem( ${-}, 0, 3 ); } catch err {}
	call assert( =( err 3 ), ${-} );
	call assert( =( $cmp( $elem( ${ 1 }, 0, 0 ) "1" ) 0 ), ${-} );
	let err = 0;
	try { call $elem( ${-}, ${ 2 }, 0 ); } catch err {}
	call assert( err, ${-} );
	call assert( =( $cmp( $child( $elem( ${()}, ${ 2 }, 0 ) ) "2" ) 0 ), ${-} );
	call assert( =( $cmp( $child( $elem( ${[]}, ${ 2 }, 0 ) ) "2" ) 0 ), ${-} );
	call assert( =( $cmp( $child( $elem( ${{}}, ${ 2 }, 0 ) ) "2" ) 0 ), ${-} );
	call assert( =( $cmp( $child( $elem( ${{}}, $elem( ${ 2 }, 0, 0 ), 0 ) ) "2" ) 0 ), ${-} );
	call assert( =( $cmp( $next( $elem( ${ 1 }, 0, ${ 3 } ) ) "3" ) 0 ), ${-} );
	call assert( =( $cmp( $next( $elem( ${ 1 }, 0, $elem( ${ 3 }, 0, 0 ) ) ) "3" ) 0 ), ${-} );
	call assert( =( $cmp( $elem( $parse( "(abc) def" ), 0, 0 ) "()" ) 0 ), ${-} );
	call assert( !( $child( $elem( $parse( "(abc) def" ), 0, 0 ) ) ), ${-} );
	call assert( =( $cmp( $child( $elem( $parse( "(abc) def" ), ${ 2 }, 0 ) ) "2" ) 0 ), ${-} );
	call assert( !( $next( $elem( $parse( "(abc) def" ), 0, 0 ) ) ), ${-} );
	call assert( =( $cmp( $next( $elem( $parse( "(abc) def" ), ${ 2 }, ${ 3 } ) ) "3" ) 0 ), ${-} );
	call assert( =( $cmp( $next( $elem( $elem( ${()}, ${ abc }, ${ def } ), ${ 2 }, ${ 3 } ) ) "3" ) 0 ), ${-} );
}

function testvalues() {
	var err, arr;
	try { call $values( except( ${ 1 } ) ); } catch err {}
	call assert( =( $cmp( $str( err ) "1" ) 0 ), ${-} );
	let err = 0;
	try { call $values( 0 ); } catch err {}
	call assert( err, ${-} );
	let err = 0;
	try { call $values( $array( 0 ) ); } catch err {}
	call assert( err, ${-} );
	let arr = $array( $parse( $unparse( $values( $array( ${ 0 1 "2" $tup( "Three" 3 ) ${ [4] } } ) ) ) ) );
	call assert( =( [ arr 0 ] 0 ), ${-} );
	call assert( =( [ arr 1 ] 1 ), ${-} );
	call assert( =( $cmp( [ arr 2 ] "2" ) 0 ), ${-} );
	call assert( =( $cmp( [ arr 3 ] $tup( "Three" 3 ) ) 0 ), ${-} );
	call assert( =( $cmp( $child( [ arr 4 ] ) "4" ) 0 ), ${-} );
}

