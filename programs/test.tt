function testworker() {
	var worker = $execute( $worker( ${ () { call $status( "1" ); return 1; } } ) );
	call assert_equals( 1, $result( worker ), ${-} );
	call assert_equals( "1", $status( worker ), ${-} );
	call assert_equals( "1", $result( $execute( $worker( ${ ( a ) { return a; } } ), "1" ) ), ${-} );
	call assert_throws( $function( ${ () { call $result( $execute( $worker( ${ ( a ) { throw a; } } ), "1" ) ); } } ), "1", ${-} );
	call assert_throws( $function( ${ () { call $worker( 0 ); } } ), "Not an element.", ${-} );
	call assert_throws( $function( ${ () { call $worker( ${ x } ); } } ), "Expected '(' after '$worker'", ${-} );
	call assert_throws( $function( ${ () { call $worker( ${ ( 1 ) {} } ); } } ), "Invalid name '1'", ${-} );
	call assert_throws( $function( ${ () { call $worker( ${ ( x x ) {} } ); } } ), "Parameter 'x' already", ${-} );
	call assert_throws( $function( ${ () { call $execute( 0 ); } } ), "Not a worker", ${-} );
	call assert_throws( $function( ${ () { call $execute( $worker( ${ ( a ) {} } ) ); } } ), "Incorrect number", ${-} );
	call assert_throws( $function( ${ () { call $execute( $worker( ${ ( a ) {} } ), ${ x } ); } } ), "Values of this type", ${-} );
	call assert_throws( $function( ${ () { call $result( 0 ); } } ), "Not a worker", ${-} );
	call assert_throws( $function( ${ () { call $result( $execute( $worker( ${ () { exit 0; } } ) ) ); } } ), "Worker exited", ${-} );
	call assert_throws( $function( ${ () { call $result( $execute( $worker( ${ () { return $array( 0 ); } } ) ) ); } } ), "Values of this type", ${-} );
	call assert_throws( $function( ${ () { call $result( $execute( $worker( ${ () { call $worker( 0 ); } } ) ) ); } } ), "Operation", ${-} );
	call assert_throws( $function( ${ () { call $status( 0 ); } } ), "Not a worker", ${-} );
	call assert_throws( $function( ${ () { var err, worker = $execute( $worker( ${ () { exit 0; } } ) );
		try { call $result( worker ); } catch err { call $status( worker ); } } } ), "Worker exited", ${-} );
	call assert_throws( $function( ${ () { var err, worker = $execute( $worker( ${ () { throw "x"; } } ) );
		try { call $result( worker ); } catch err { call $status( worker ); } } } ), "x", ${-} );
}

