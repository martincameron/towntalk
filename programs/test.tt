function testwhile() {
	var err, n;
	try { while except( ${-} ) { let err = 0; } } catch err {}
	call assert( err, ${-} );
	while <( n 1 ) { inc n; }
	call assert( =( n 1 ), ${-} );
	while <( n 10 ) {
		break;
		call assert( FALSE, ${-} );
	}
	while <( n 10 ) {
		if =( n 5 ) { break; }
		inc n;
		continue;
		call assert( FALSE, ${-} );
	}
	call assert( =( n 5 ), ${-} );
	while TRUE {
		try { break; } catch err {}
		call assert( FALSE, ${-} );
	}
	while TRUE {
		try { throw 0; } catch err { break; }
		call assert( FALSE, ${-} );
	}
	while TRUE {
		return 0;
		call assert( FALSE, ${-} );
	}
	var off, end = 10;
	while <( off end ) {
		var aaa, bbb = 0;
		inc aaa;
		inc bbb;
		inc off;
	}
	call assert( =( off 10 ), ${-} );
	call assert( =( aaa 10 ), ${-} );
	call assert( =( bbb 1 ), ${-} );
}

