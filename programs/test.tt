	call assert_equals( "3", $next( $elem( ${ 1 }, 0, ${ 3 } ) ), ${-} );
	call assert_equals( "3", $next( $elem( ${ 1 }, 0, $elem( ${ 3 }, 0, 0 ) ) ), ${-} );
	call assert_equals( "()", $elem( $parse( "(abc) def" ), 0, 0 ), ${-} );
	call assert_equals( 0, $child( $elem( $parse( "(abc) def" ), 0, 0 ) ), ${-} );
	call assert_equals( "2", $child( $elem( $parse( "(abc) def" ), ${ 2 }, 0 ) ), ${-} );
	call assert_equals( 0, $next( $elem( $parse( "(abc) def" ), 0, 0 ) ), ${-} );
	call assert_equals( "3", $next( $elem( $parse( "(abc) def" ), ${ 2 }, ${ 3 } ) ), ${-} );
	call assert_equals( "3", $next( $elem( $elem( ${()}, ${ abc }, ${ def } ), ${ 2 }, ${ 3 } ) ), ${-} );
	call assert_throws( $function( ${ () { call $elem( except( ${ 1 } ), 0, 0 ); } } ), "1", ${-} );
	call assert_throws( $function( ${ () { call $elem( 1, 0, 0 ); } } ), "Not an element.", ${-} );
	call assert_throws( $function( ${ () { call $elem( ${-}, except( ${ 2 } ), 0 ); } } ), "2", ${-} );
	call assert_throws( $function( ${ () { call $elem( ${-}, 2, 0 ); } } ), "Not an element.", ${-} );
	call assert_throws( $function( ${ () { call $elem( ${-}, 0, except( ${ 3 } ) ); } } ), "3", ${-} );
	call assert_throws( $function( ${ () { call $elem( ${-}, 0, 3 ); } } ), "Not an element.", ${-} );
	call assert_throws( $function( ${ () { call $elem( ${-}, ${ 2 }, 0 ); } } ), "Parent elements must", ${-} );
